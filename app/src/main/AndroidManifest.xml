<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.progdawn.visitcolumbusga">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Heritage"
            android:label="@string/title_activity_heritage" />
        <activity
            android:name=".Homeless"
            android:label="@string/title_activity_homeless" />
        <activity
            android:name=".River"
            android:label="@string/title_activity_river"></activity>
    </application>

</manifest>